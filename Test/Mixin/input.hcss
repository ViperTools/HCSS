@mixin dark {
    background: gray;
    color: white;
}

@mixin noselect {
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}

@mixin last {
    margin: 0;
}

.test {
    @include dark, noselect;
    color: red;
    @include last;
}